inner_hclose: [WS] "inner_hclose" NUMBER _NL+
inner_dvclose: [WS] "inner_dvclose" NUMBER _NL+
rcloserecord: [WS] "rcloserecord" NUMBER word _NL+
linear_acceleration: [WS] "linear_acceleration" word _NL+
relaxation_factor: [WS] "relaxation_factor" NUMBER _NL+
preconditioner_levels: [WS] "preconditioner_levels" NUMBER _NL+
preconditioner_drop_tolerance: [WS] "preconditioner_drop_tolerance" NUMBER _NL+
number_orthogonalizations: [WS] "number_orthogonalizations" NUMBER _NL+
