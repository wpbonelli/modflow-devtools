// Generated grammar for MF6 GWE-SSM component
%import "base.lark"

start: [WS] [_NL*] (block [[WS] _NL*])+ [WS]
block: options_block | sources_block | fileinput_block
options_block: "begin"i "options" [_block_index] _NL options_content "end"i "options" [_block_index] _NL+
options_content: (print_flows | save_flows)*
sources_block: "begin"i "sources" [_block_index] _NL sources_content "end"i "sources" [_block_index] _NL+
sources_content: (sources)*
fileinput_block: "begin"i "fileinput" [_block_index] _NL fileinput_content "end"i "fileinput" [_block_index] _NL+
fileinput_content: (fileinput)*
print_flows: [WS] "print_flows" _NL+
save_flows: [WS] "save_flows" _NL+
sources: [WS] "sources" word* _NL+
fileinput: [WS] "fileinput" word* _NL+
