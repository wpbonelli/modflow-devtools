// Generated grammar for MF6 GWT-FMI component
%import "base.lark"

start: [WS] [_NL*] (block [[WS] _NL*])+ [WS]
block: options_block | packagedata_block
options_block: "begin"i "options" [_block_index] _NL options_content "end"i "options" [_block_index] _NL+
options_content: (save_flows | flow_imbalance_correction)*
packagedata_block: "begin"i "packagedata" [_block_index] _NL packagedata_content "end"i "packagedata" [_block_index] _NL+
packagedata_content: (packagedata)*
save_flows: [WS] "save_flows" _NL+
flow_imbalance_correction: [WS] "flow_imbalance_correction" _NL+
packagedata: [WS] "packagedata" word* _NL+
