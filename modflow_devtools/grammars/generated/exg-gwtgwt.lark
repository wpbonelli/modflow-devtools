// Generated grammar for MF6 EXG-GWTGWT component
%import "base.lark"

start: [WS] [_NL*] (block [[WS] _NL*])+ [WS]
block: options_block | dimensions_block | exchangedata_block
options_block: "begin"i "options" [_block_index] _NL options_content "end"i "options" [_block_index] _NL+
options_content: (gwfmodelname1 | gwfmodelname2 | auxiliary | boundnames | print_input | print_flows | save_flows | adv_scheme | dsp_xt3d_off | dsp_xt3d_rhs | perioddata | observations | dev_interfacemodel_on)*
dimensions_block: "begin"i "dimensions" [_block_index] _NL dimensions_content "end"i "dimensions" [_block_index] _NL+
dimensions_content: (nexg)*
exchangedata_block: "begin"i "exchangedata" [_block_index] _NL exchangedata_content "end"i "exchangedata" [_block_index] _NL+
exchangedata_content: (exchangedata)*
gwfmodelname1: [WS] "gwfmodelname1" word _NL+
gwfmodelname2: [WS] "gwfmodelname2" word _NL+
auxiliary: [WS] "auxiliary" word _NL+
boundnames: [WS] "boundnames" _NL+
print_input: [WS] "print_input" _NL+
print_flows: [WS] "print_flows" _NL+
save_flows: [WS] "save_flows" _NL+
adv_scheme: [WS] "adv_scheme" word _NL+
dsp_xt3d_off: [WS] "dsp_xt3d_off" _NL+
dsp_xt3d_rhs: [WS] "dsp_xt3d_rhs" _NL+
perioddata: [WS] "perioddata" word* _NL+
observations: [WS] "observations" word* _NL+
dev_interfacemodel_on: [WS] "dev_interfacemodel_on" _NL+
nexg: [WS] "nexg" NUMBER _NL+
exchangedata: [WS] "exchangedata" array_format _NL+ array_data
