// Generated grammar for MF6 GWT-DISU component
%import "base.lark"

start: [WS] [_NL*] (block [[WS] _NL*])+ [WS]
block: options_block | dimensions_block | griddata_block | connectiondata_block | vertices_block | cell2d_block
options_block: "begin"i "options" [_block_index] _NL options_content "end"i "options" [_block_index] _NL+
options_content: (length_units | nogrb | grb_filerecord | xorigin | yorigin | angrot | vertical_offset_tolerance | export_array_ascii | crs)*
dimensions_block: "begin"i "dimensions" [_block_index] _NL dimensions_content "end"i "dimensions" [_block_index] _NL+
dimensions_content: (nodes | nja | nvert)*
griddata_block: "begin"i "griddata" [_block_index] _NL griddata_content "end"i "griddata" [_block_index] _NL+
griddata_content: (top | bot | area | idomain)*
connectiondata_block: "begin"i "connectiondata" [_block_index] _NL connectiondata_content "end"i "connectiondata" [_block_index] _NL+
connectiondata_content: recarray_line*
vertices_block: "begin"i "vertices" [_block_index] _NL vertices_content "end"i "vertices" [_block_index] _NL+
vertices_content: recarray_line*
cell2d_block: "begin"i "cell2d" [_block_index] _NL cell2d_content "end"i "cell2d" [_block_index] _NL+
cell2d_content: (cell2d)*
length_units: [WS] "length_units" word _NL+
nogrb: [WS] "nogrb" _NL+
grb_filerecord: [WS] "grb_filerecord" word word word _NL+
xorigin: [WS] "xorigin" NUMBER _NL+
yorigin: [WS] "yorigin" NUMBER _NL+
angrot: [WS] "angrot" NUMBER _NL+
vertical_offset_tolerance: [WS] "vertical_offset_tolerance" NUMBER _NL+
export_array_ascii: [WS] "export_array_ascii" _NL+
crs: [WS] "crs" word _NL+
nodes: [WS] "nodes" NUMBER _NL+
nja: [WS] "nja" NUMBER _NL+
nvert: [WS] "nvert" NUMBER _NL+
top: [WS] "top" NUMBER _NL+
bot: [WS] "bot" NUMBER _NL+
area: [WS] "area" NUMBER _NL+
idomain: [WS] "idomain" NUMBER _NL+
cell2d: [WS] "cell2d" array_format _NL+ array_data
